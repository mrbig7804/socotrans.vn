    <script type="text/javascript">
        $(function () {
            $(window).scroll(function () {
                if ($(this).scrollTop() != 0) {
                    $('#to_top').fadeIn();
                } else {
                    $('#to_top').fadeOut();
                }
            });

            $('#to_top').click(function () {
                $('body,html').animate({ scrollTop: 0 }, 600);
            });

            //var api = $(".shiner").peShiner({ api: true, paused: true, reverse: true, repeat: 1, color: 'white' });
            //api.resume();

            $(function () {
                $('a.lightbox').lightBox(); // Select all links with lightbox class
            });
        });
    </script>
