    <script>
        $(document).ready(function () {
            var raido = $(".wrapFaceMss").attr("data-toggle");
            if (raido == 0) {
                $(".mnv-tuvan").css("display", "none");
                $(".x").click(function () {
                    $(".wrapFaceMss").slideToggle();
                    $(".mnv-tuvan").slideToggle();
                });

                $(".mnv-tuvan").click(function () {
                    $(".wrapFaceMss").slideToggle();
                    $(this).slideToggle();
                });
            }
            else {
                $(".wrapFaceMss").css("display", "none");
                $(".x").click(function () {
                    $(".wrapFaceMss").slideToggle();
                    $(".mnv-tuvan").slideToggle();
                });
                $(".mnv-tuvan").click(function () {
                    $(".wrapFaceMss").slideToggle();
                    $(this).slideToggle();
                });
            }
        })
    </script>

